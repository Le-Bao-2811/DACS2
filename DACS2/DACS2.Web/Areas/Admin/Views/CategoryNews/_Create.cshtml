@model DACS2.Web.Areas.Admin.ViewModel.NewsCategory.AddorUpdateCategoryNewsVM


<div class="row p-3">
    <div class="col-md-8">
        <form asp-action="_Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group" hidden>
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control id-category" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProductNewsName" class="control-label"></label>
                <input asp-for="ProductNewsName" class="form-control" />
                <span asp-validation-for="ProductNewsName" class="text-danger"></span>
            </div>
        </form>
    </div>
</div>



