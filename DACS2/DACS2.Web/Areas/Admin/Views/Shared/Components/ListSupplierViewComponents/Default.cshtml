@model IEnumerable<DACS2.Data.Entities.Supplier>

@foreach (var item in Model)
{
    if (ViewBag.selected != null)
    {
        if (ViewBag.selected == item.Id)
        {
            <option value="@item.Id" selected>@item.SupplierName</option>
        }
        else
        {
            <option value="@item.Id">@item.SupplierName</option>
        }
    }
    else
    {
        <option value="@item.Id">@item.SupplierName</option>
    }
}